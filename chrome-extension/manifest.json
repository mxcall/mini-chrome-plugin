{
  "manifest_version": 3,
  "name": "文件上传插件",
  "version": "1.0.0",
  "description": "一个简单的文件上传Chrome插件,可将文件上传到本地服务器",
  "permissions": [],
  "host_permissions": [
    "http://localhost:19666/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
